package test;

public class Time {
int hour;
int minute;
int second;
public Time(int hour, int minute, int second) {
	
}
public void setTime(int hour, int minute, int second){
	if(hour > 23 || hour<0) {
		hour=0;
	}
	if(minute > 59 || minute<0) {
		minute=0;
	}
	if(second > 23 || second<0) {
		second=0;
	}
	this.hour =hour;
	this.minute=minute;
	this.second=second;
}
}