package test;

public class Score {
	private int kor;
	private int eng;
	private int math;
	public Score(int kor, int eng, int math) {
		this.kor=kor;
		this.eng=eng;
		this.math=math;
	}
	public int getKor() {
		return this.kor;
	}
	public int getEng() {
		return this.eng;
	}
	public int getMath() {
		return this.math;
	}
	public void setKor(int kor) {
		this.kor=kor;
	}
	public void setEng(int eng) {
		this.eng=eng;
	}
	public void setMath(int math) {
		this.math=math;
}
	public  String toString() {
		return "점수 : 국어 : " + kor + ", 영어 : " + eng + ", 수학 : " + math + "]";
	}
}
